@model IEnumerable<DepoYonetimSistemi.Models.Depo>

@{
    Layout = "~/Views/Shared/AdminSayfalari.cshtml";
}

@foreach (var depo in Model)
{
    <form action="/Urun/DepoUrunListe" method="post">
        <button type="submit" name="DepoAdi" value="@depo.DepoAdi">@depo.DepoAdi</button><br><br>
    </form>
}
